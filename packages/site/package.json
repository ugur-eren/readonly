{
  "name": "site",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "yarn@3.6.3",
  "scripts": {
    "allow-scripts": "yarn workspace root allow-scripts",
    "start": "dotenv -e ../../.env vite",
    "build": "tsc && dotenv -e ../../.env vite build",
    "preview": "dotenv -e ../../.env vite preview",
    "test": "dotenv -e ../../.env vite test",
    "format:check": "prettier -c \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint:check": "eslint \"src/**/*.{ts,tsx}\"",
    "lint": "yarn lint:ts:check --fix",
    "ts:check": "tsc --noEmit",
    "check": "yarn format && yarn format:check && yarn lint:check && yarn ts:check"
  },
  "dependencies": {
    "@metamask/keyring-api": "^1.1.0",
    "@metamask/providers": "^13.0.0",
    "@tanstack/react-query": "^5.17.10",
    "bootstrap": "^5.1.3",
    "classnames": "^2.3.1",
    "common": "*",
    "contracts": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-responsive": "^9.0.0-beta.6",
    "react-router": "^6.18.0",
    "react-router-dom": "^6.18.0",
    "viem": "^2.0.6",
    "wagmi": "^2.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.17",
    "babel-eslint": "^10.1.0",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.7.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "postcss": "^8.4.33",
    "postcss-import": "^16.0.0",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.5"
  }
}
